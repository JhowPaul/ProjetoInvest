<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.projetoinvest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/stockwealthlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/stockwealthlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Projetoinvest">
        <activity
            android:name=".activity.activity.SplashScreenActivity"
            android:exported="false" />
        <activity
            android:name=".activity.activity.MainActivity"
            android:exported="false" />
        <activity
            android:name=".activity.activity.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>